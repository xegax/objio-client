{
  "name": "ts-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "repository": "git@github.com:xegax/ts-boilerplate.git",
  "scripts": {
    "dist": "npm run externals && npm run entry",
    "entry": "webpack",
    "externals": "webpack --config webpack.externals.js",
    "dev": "npm run tsc && npm run sass",
    "tsc": "tsc",
    "sass": "node_modules/.bin/node-sass styles/styles.scss > build/styles.css",
    "watch": "tsc --watch",
    "clean": "cd src && rm -r ./*/*.js ./*/*.js.map",
    "test-node": "node_modules/.bin/jasmine --config=jasmine-node.json",
    "karma": "node node_modules/karma/bin/karma start karma.conf.js",
    "test": "npm run dev && npm run karma"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/d3": "^4.9.0",
    "@types/es6-shim": "^0.31.34",
    "@types/jasmine": "^2.5.53",
    "@types/react": "^16.3.12",
    "@types/react-dom": "^16.0.5",
    "awesome-typescript-loader": "^3.2.1",
    "axios": "^0.17.1",
    "bluebird": "^3.5.0",
    "css-loader": "^0.28.11",
    "d3": "^4.10.0",
    "font-awesome": "^4.7.0",
    "jasmine": "^2.6.0",
    "jasmine-core": "^2.5.2",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-requirejs": "^1.1.0",
    "karma-spec-reporter": "0.0.32",
    "node-sass": "^4.7.2",
    "requirejs": "^2.3.5",
    "ts-loader": "^4.2.0",
    "typescript": "^2.4.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15"
  },
  "dependencies": {
    "@blueprintjs/core": "^2.2.1",
    "@types/react-transition-group": "^2.0.8",
    "compression-webpack-plugin": "^1.1.11",
    "file-loader": "^1.1.11",
    "normalize.css": "^8.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.0.0",
    "react-transition-group": "^2.3.1",
    "style-loader": "^0.21.0",
    "tslint": "^3.15.1",
    "url-loader": "^1.0.1",
    "url-resolve": "^1.0.0",
    "webpack-encoding-plugin": "^0.2.1"
  }
