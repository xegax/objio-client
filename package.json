  "scripts": {
    "prepare": "npm run dist",
    "dist": "npm run externals && npm run entry-dist && npm run server-dist",
    "dev": "npm run entry && npm run server-dev",
    "entry": "webpack",
    "entry-dist": "webpack -p",
    "externals": "webpack -p --config webpack.externals.js",
    "server": "node server/server",
    "server-dev": "tsc -p tsconfig-srv.json",
    "server-dist": "webpack --config webpack.server.js",
    "tsc": "tsc",
    "sass": "node_modules/.bin/node-sass styles/styles.scss > build/styles.css",
    "watch": "tsc --watch",
    "clean": "shx rm -rf src/**/*.js src/**/*.js.map",
    "test-node": "node_modules/.bin/jasmine --config=jasmine-node.json",
    "karma": "node node_modules/karma/bin/karma start karma.conf.js",
    "test": "npm run dev && npm run karma"
  },
